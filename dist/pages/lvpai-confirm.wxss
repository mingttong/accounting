.container {
  padding: 0 30rpx;
}

.container .box {
  margin-top: 30rpx;
  padding: 36rpx 26rpx;
  -webkit-box-shadow: 0 1px 10px 0 rgba(0, 0, 0, 0.1);
          box-shadow: 0 1px 10px 0 rgba(0, 0, 0, 0.1);
  background-color: #fff;
  border-radius: 8rpx;
}

.container .box .top .title {
  font-size: 30rpx;
}

.container .box .top .time {
  font-size: 26rpx;
  color: #999;
}

.container .box .price {
  border-bottom: 1rpx dashed #f3f3f3;
  text-align: center;
  padding: 30rpx 0;
}

.container .box .price .label {
  font-size: 28rpx;
  color: #999;
}

.container .box .price .value {
  font-size: 60rpx;
}

.container .box .info {
  margin-top: 30rpx;
}

.container .box .info .section {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  margin-bottom: 18rpx;
}

.container .box .info .section .label {
  color: #999;
  margin-right: 20rpx;
  width: 150rpx;
}

.container .pay {
  background-color: #4e88ff;
  width: 690rpx;
  border-radius: 4rpx;
  color: #FFF;
  font-size: 34rpx;
  margin-top: 120rpx;
}

.icon-see-more {
  background-image: url("data:image/png;base64,");
  width: 26rpx;
  height: 26rpx;
  background-size: 26rpx 26rpx;
}

.icon-star {
  background-image: url("data:image/png;base64,");
  width: 26rpx;
  height: 26rpx;
  background-size: 26rpx 26rpx;
}
