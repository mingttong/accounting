.last {
  border-bottom: none !important;
}

.container {
  height: 100%;
}

.container .outgo {
  height: 100%;
}

.container .outgo .type-list {
  height: 100%;
}

.container .outgo .subtype-list {
  height: 100%;
}

.outgo {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.outgo .type-list {
  width: 180rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}

.outgo .type-list .item {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}

.outgo .type-list .item.active {
  background-color: #fff;
  position: relative;
}

.outgo .type-list .item.active::after {
  display: block;
  content: '';
  position: absolute;
  width: 8rpx;
  height: 100rpx;
  top: 0;
  left: 0;
  background-color: #d74c45;
}

.outgo .subtype-list {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background-color: #fff;
  padding-left: 30rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
}

.outgo .subtype-list .item {
  position: relative;
  border-bottom: 1rpx solid #ccc;
  height: 100rpx;
  line-height: 100rpx;
}

.outgo .subtype-list .item .icon-star {
  position: absolute;
  display: block;
  content: '';
  width: 100rpx;
  height: 100rpx;
  background-image: url("data:image/png;base64,");
  background-size: 48rpx 48rpx;
  background-repeat: no-repeat;
  background-position: top 26rpx right 26rpx;
  top: 0;
  right: 0;
}

.outgo .subtype-list .item .icon-star.star {
  background-image: url("data:image/png;base64,");
}

.income .type-list {
  padding-left: 30rpx;
}

.income .type-list .item {
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #ccc;
}
